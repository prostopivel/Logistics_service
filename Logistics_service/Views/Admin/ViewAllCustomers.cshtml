@using System;
@using Logistics_service.Models.Users;
@model List<Customer>

<title>ViewAllCustomers</title>
<link rel="stylesheet" href="styles.css">

<div class="container">
    <h1>Список пользователей</h1>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Email</th>
                <th>Role</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.Email</td>
                    <td>@item.Role</td>
                </tr>
            }
        </tbody>
    </table>
    <button onclick="auth(`GET`, `/dashboard/administrator`)">Назад</button>
</div>
